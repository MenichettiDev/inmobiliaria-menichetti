@model InmobiliariaApp.Models.TipoInmueble

@{
    ViewData["Title"] = "Detalles del Tipo de Inmueble";
}

<h1>Detalles del Tipo de Inmueble</h1>

<div>
    <h4>TipoInmueble</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">ID</dt>
        <dd class="col-sm-10">@Model.IdTipoInmueble</dd>

        <dt class="col-sm-2">Nombre</dt>
        <dd class="col-sm-10">@Model.Nombre</dd>
    </dl>
</div>
<div class="mt-3">
    <a asp-action="Editar" asp-route-id="@Model.IdTipoInmueble" class="btn btn-warning">Editar</a>
    <a asp-action="Listar" class="btn btn-secondary">Volver</a>
</div>
